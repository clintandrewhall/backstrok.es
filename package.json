{
  "name": "backstrok.es",
  "description": "A Web Application, written in Node.js, containing a collection of social media questions I've had that I want to answer.",
  "version": "0.0.3",
  "author": "Clint Andrew Hall <clint@clintandrewhall.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/clintandrewhall/backstrok.es"
  },
  "bin": "./server.js",
  "scripts": {
    "babel": "babel src -d dist",
    "browserify": "browserify --extension=jsx src/public/scripts/index.js -o src/public/scripts/bundle.js",
    "build": "npm run browserify && npm run babel",
    "start": "node dist/server.js local"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-merge-sibling-variables": "^6.8.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.61.0",
    "jest": "^22.0.3",
    "prettier": "^1.9.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "connect": "^3.6.5",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-react-views": "^0.10.4",
    "express-session": "^1.15.6",
    "moment": "^2.20.1",
    "node-foursquare": "^0.3.2",
    "node-jsx": "^0.13.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-engine": "^4.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "relative-date": "^1.1.3",
    "underscore": "^1.8.3",
    "winston": "^2.4.0"
  },
  "subdomain": "backstrokes",
  "engines": {
    "node": "0.12.x"
  },
  "browserify": {
    "extensions": [
      "jsx"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "env"
            ],
            [
              "react"
            ],
            [
              "flow"
            ]
          ]
        }
      ]
    ]
  }
}
