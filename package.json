{
  "name": "backstrok.es",
  "description": "A Web Application, written in Node.js, containing a collection of social media questions I've had that I want to answer.",
  "version": "0.0.3",
  "author": "Clint Andrew Hall <clint@clintandrewhall.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/clintandrewhall/backstrok.es"
  },
  "dependencies": {
    "express-session": "^1.15.6",
    "flow-bin": "^0.62.0",
    "moment": "^2.20.1",
    "node-foursquare": "^0.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "relative-date": "^1.1.3",
    "underscore": "^1.8.3",
    "winston": "^2.4.0"
  },
  "scripts": {
    "build-react": "react-scripts build",
    "build-server": "babel server/ --out-dir build/server",
